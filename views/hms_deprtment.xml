<odoo>
<data>
<record id="departmant_action" model="ir.actions.act_window">
<field name="name">Departments</field>
<field name="res_model">hms.deprtmant</field>
</record>

<record id="department_form" model="ir.ui.view">
<field name="name">hms department form</field>
<field name="model">hms.deprtmant</field>
<field name="arch" type="xml">
<form string="deprtmant">
<sheet>
<group>
<field name="name"/>
<field name="capacity"/>
<field name="is_oppened"/>
</group>
</sheet>
</form>
</field>
</record>

<menuitem  name="department" id="department_menu" parent="hms_menu" action="departmant_action"/>
</data>
</odoo>